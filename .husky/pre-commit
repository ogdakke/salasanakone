if pnpm test -- --run --coverage; then
  printf "\nTests ran successfully\n" 
else
  printf "\nTests FAILED...\n\n"
  exit 1
fi;

if pnpm lint; then
  printf "\nLinting ran successfully\n"
else
  printf "\nLinting FAILED, running format:write...\n"
  pnpm format:write && printf "\nRan formatter, try commiting again.\n\n"
  exit 1
fi